    "en-US": {
      "language": "en-US",
      "languageName": "English",
      "lessonName": "Checking in at the Hotel",
      "exercises": [
        {
          "id": 0,
          "type": "narration",
          "text": "You have arrived at your hotel and are getting ready to check in."
        },
        {
          "id": 1,
          "type": "prompt",
          "character": {
            "name": "Receptionist",
            "svg": 1
          },
          "text": "Good morning. Are you ready to check in?",
          "hint": "Yes, ...",
          "expectedAnswers": [
            "Yes, I am ready to check in.",
            "Yes, I'm ready to check in."
          ]
        },
        {
          "id": 2,
          "type": "response",
          "character": {
            "name": "You",
            "svg": 2
          }
        },
        {
          "id": 3,
          "type": "prompt",
          "character": {
            "name": "Receptionist",
            "svg": 1
          },
          "text": "Great! Can you show me some identification?",
          "hint": "Yes, ... my passport",
          "expectedAnswers": [
            "Yes, I can show you my passport.",
            "Yes, here is my passport."
          ]
        },
        {
          "id": 4,
          "type": "response",
          "character": {
            "name": "You",
            "svg": 2
          }
        },
        {
          "id": 5,
          "type": "prompt",
          "character": {
            "name": "Receptionist",
            "svg": 1
          },
          "text": "Thank you. How many nights will you be staying here?",
          "hint": "... three nights",
          "expectedAnswers": [
            "I will be staying here for three nights.",
            "I'll be staying here for three nights."
          ]
        },
        {
          "id": 6,
          "type": "response",
          "character": {
            "name": "You",
            "svg": 2
          }
        },
        {
          "id": 7,
          "type": "prompt",
          "character": {
            "name": "Receptionist",
            "svg": 1
          },
          "text": "Very well. Here is your room key. You'll be staying in room 305. Do you have any questions?",
          "hint": "Yes, what time ... breakfast ... served?",
          "expectedAnswers": [
            "Yes, what time will breakfast be served?",
            "Yes, what time is breakfast served?"
          ]
        },
        {
          "id": 8,
          "type": "response",
          "character": {
            "name": "You",
            "svg": 2
          }
        },
        {
          "id": 10,
          "type": "prompt",
          "character": {
            "name": "Receptionist",
            "svg": 1
          },
          "text": "Breakfast will be served at 9:00 on the first floor. Anything else?",
          "hint": "Yes, ... WiFi ... my room?",
          "expectedAnswers": [
            "Yes, is WiFi available in my room?",
            "Yes, is there WiFi available in my room?"
          ]
        },
        {
          "id": 11,
          "type": "response",
          "character": {
            "name": "You",
            "svg": 2
          }
        },
        {
          "id": 12,
          "type": "prompt",
          "character": {
            "name": "Receptionist",
            "svg": 1
          },
          "text": "Yes, of course. The WiFi password is written on your room key. Now, how would you like to pay?",
          "hint": "... Visa",
          "expectedAnswers": [
            "I would like to pay by Visa.",
            "I'd like to pay by Visa."
          ]
        },
        {
          "id": 13,
          "type": "response",
          "character": {
            "name": "You",
            "svg": 2
          }
        },
        {
          "id": 14,
          "type": "prompt",
          "character": {
            "name": "Receptionist",
            "svg": 1
          },
          "text": "Perfect, thank you. Your check-in is complete. Enjoy your stay!",
          "hint": "... very much",
          "expectedAnswers": [
            "Thank you very much.",
            "Thanks very much."
          ]
        },
        {
          "id": 15,
          "type": "response",
          "character": {
            "name": "You",
            "svg": 2
          }
        },
        {
          "id": 16,
          "type": "narration",
          "text": "Well done! You have successfully checked in, and are ready to go up to your room now."
        }
      ]
    },
    "fr-FR": {
