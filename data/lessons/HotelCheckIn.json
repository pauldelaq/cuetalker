          "type": "prompt",
          "character": {
            "name": "Réceptionniste",
            "svg": 1
          },
          "text": "Très bien ! Pouvez-vous me montrer une pièce d'identité ?",
          "hint": "Oui, ... mon passeport",
          "expectedAnswers": [
            "Oui, je peux vous montrer mon passeport.",
            "Oui, voici mon passeport."
